{
  "name": "kodomogakuen-react",
  "version": "1.0.0",
  "description": "# Context Engineering Template",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npm run validate-content && next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "validate-content": "node scripts/validate-content.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "clean": "rm -rf .next && npm cache clean --force",
    "clean:all": "rm -rf .next node_modules && npm cache clean --force",
    "fresh-install": "npm run clean:all && npm install",
    "disk-check": "du -sh node_modules .next public/images 2>/dev/null || echo 'Checking disk usage...'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badjonz/kodomo-gakuen-context-repo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/badjonz/kodomo-gakuen-context-repo/issues"
  },
  "homepage": "https://github.com/badjonz/kodomo-gakuen-context-repo#readme",
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "framer-motion": "^12.23.12",
    "next": "^15.4.5",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "^1.90.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
